    "devnet:test": "ts-node scripts/test-devnet.ts",
    "devnet:airdrop": "solana airdrop 2 --url https://api.devnet.solana.com",
    "devnet:balance": "solana balance --url https://api.devnet.solana.com",
    "devnet:config": "solana config get",
    "devnet:explorer": "echo 'https://explorer.solana.com/address/'$(solana-keygen pubkey /Users/user2/.config/solana/basalt.json)'?cluster=devnet'"
