{
  "name": "basalt-cdp-mvp-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:devnet": "REACT_APP_SOLANA_CLUSTER=devnet vite",
    "dev:localhost": "REACT_APP_SOLANA_CLUSTER=localhost vite",
    "dev:testnet": "REACT_APP_SOLANA_CLUSTER=testnet vite",
    "build": "vite build",
    "build:devnet": "REACT_APP_SOLANA_CLUSTER=devnet vite build",
